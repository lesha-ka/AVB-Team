                a(href="about.html").header__link 
                    .link-text Über uns
                a(href="product.html").header__link 
                    .link-text Honorarberatung
                a(href="news-list.html").header__link.active
                    .link-text Aktuelles
                a(href="contacts.html").header__link 
                    .link-text Kontakt
            ul.header__controls 
                li.header__control 
                    img(src="img/icons/search.svg", alt="search")
                li.header__control 
                    img(src="img/icons/phone.svg", alt="phone")
                li.header__control 
                    img(src="img/icons/user.svg", alt="user")

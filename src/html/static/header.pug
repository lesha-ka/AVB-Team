header.header
    .container 
        .header-inner 
            a(href="index.html").header__logo
                img(src="img/icons/logo.svg", alt="AVB Team")
            .header__menu.js-menuOpen
                .header__new NEU 
                .header__opener Planungsmodule
                .header__menu-list.js-menuList 
                    a(href="#").header__menu-item 
                        .header__menu-img 
                            img(src="img/icons/starhalf.svg", alt="")
